{
  "name": "goto-char-timer",
  "displayName": "GotoCharTimer",
  "description": "",
  "version": "0.0.16",
  "publisher": "rgkirch",
  "license": "MIT",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/rgkirch/goto-char-timer.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "extension.gotoCharTimer",
        "title": "goto char timer"
      }
    ],
    "keybindings": [
      {
        "command": "extension.gotoCharTimer",
        "key": "alt+d"
      }
    ],
    "configuration": {
      "title": "Goto Char Timer",
      "type": "object",
      "properties": {
        "gotoCharTimer.charset": {
          "type": "string",
          "default": "abcdefghijklmnopqrstuvwxyz"
        },
        "gotoCharTimer.timeout": {
          "type": "number",
          "default": 800
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "bundle": "node esbuild.js",
    "publish": "vsce package && vsce publish"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "@vscode/vsce": "^3.2.1",
    "circular-dependency-plugin": "^5.2.2",
    "clean-webpack-plugin": "^4.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "event-stream": "^4.0.1",
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "gulp": "^5.0.0",
    "gulp-bump": "^3.2.0",
    "gulp-git": "^2.11.0",
    "gulp-tag-version": "^1.3.1",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^9.1.7",
    "npm-run-all": "^4.1.5",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "rxjs": "^7.8.1"
  },
  "lint-staged": {
    "*.{ts,js,json,md,yml}": "prettier --write",
    "*.ts": "eslint --fix"
  }
}
